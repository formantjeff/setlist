{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "pages/api/lyrics.js": {
      "maxDuration": 10
    },
    "pages/api/test.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}